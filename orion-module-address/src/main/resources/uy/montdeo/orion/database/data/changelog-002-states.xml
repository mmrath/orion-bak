<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd
    http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">
       
    <!-- STATES -->
    <changeSet id="insert-states" author="fabian.lobo">
		<preConditions onFail="MARK_RAN">
			<and>
				<tableExists tableName="${table.prefix}_STATE" />
				<tableIsEmpty tableName="${table.prefix}_STATE" />
			</and>
		</preConditions>
		
		<sql endDelimiter=";">
			INSERT INTO ${table.prefix}_STATE(
				${table.prefix}_CODE, 
				${table.prefix}_COUNTRY_ID, 
				${table.prefix}_VERSION,
				${table.prefix}_CREATED_BY,
				${table.prefix}_LAST_MODIFIED_BY
			) VALUES
				('AR', ${id.country.uy}, 0, '${value.user}', '${value.user}'),
				('CA', ${id.country.uy}, 0, '${value.user}', '${value.user}'),
				('CE', ${id.country.uy}, 0, '${value.user}', '${value.user}'),
				('CO', ${id.country.uy}, 0, '${value.user}', '${value.user}'),
				('DU', ${id.country.uy}, 0, '${value.user}', '${value.user}'),
				('FL', ${id.country.uy}, 0, '${value.user}', '${value.user}'),
				('FD', ${id.country.uy}, 0, '${value.user}', '${value.user}'),
				('LA', ${id.country.uy}, 0, '${value.user}', '${value.user}'),
				('MA', ${id.country.uy}, 0, '${value.user}', '${value.user}'),
				('MO', ${id.country.uy}, 0, '${value.user}', '${value.user}'),
				('PA', ${id.country.uy}, 0, '${value.user}', '${value.user}'),
				('RN', ${id.country.uy}, 0, '${value.user}', '${value.user}'),
				('RI', ${id.country.uy}, 0, '${value.user}', '${value.user}'),
				('RO', ${id.country.uy}, 0, '${value.user}', '${value.user}'),
				('SA', ${id.country.uy}, 0, '${value.user}', '${value.user}'),
				('SJ', ${id.country.uy}, 0, '${value.user}', '${value.user}'),
				('SO', ${id.country.uy}, 0, '${value.user}', '${value.user}'),
				('TA', ${id.country.uy}, 0, '${value.user}', '${value.user}'),
				('TT', ${id.country.uy}, 0, '${value.user}', '${value.user}');
		</sql>
		
	</changeSet>
	
 </databaseChangeLog>